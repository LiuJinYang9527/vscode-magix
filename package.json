{
  "name": "mx-plugin",
  "displayName": "mx-plugin",
  "description": "This is the README for your extension \"mx-plugin\". After writing up a brief description, we recommend including the following sections.",
  "version": "0.0.5",
  "publisher": "zhuori",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mx.plugin",
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "mx.jumper.toHtml",
        "title": "magix-跳转至html"
      },
      {
        "command": "mx.jumper.toES",
        "title": "magix-跳转至js/ts"
      },
      {
        "command": "mx.jumper.backAndForth",
        "title": "MX"
      },
      {
        "command": "mx.test",
        "title": "MX Test"
      }
    ],
    "keybindings": [
      {
        "command": "mx.jumper.backAndForth",
        "key": "alt+tab",
        "mac": "alt+tab",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId ==  typescript",
          "command": "mx.jumper.toHtml",
          "group": "0_mx"
        },
        {
          "when": "resourceLangId ==  javascript",
          "command": "mx.jumper.toHtml",
          "group": "0_mx"
        },
        {
          "when": "resourceLangId ==  html",
          "command": "mx.jumper.toES",
          "group": "0_mx"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "beautifulGirl",
          "title": "mx-plugin",
          "icon": "images/Magix.svg"
        }
      ]
    },
    "views": {
      "beautifulGirl": [
        {
          "id": "beautifulGirl1",
          "name": "插件设置"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "@types/parse5": "^5.0.0",
    "babylon": "^6.18.0",
    "map-lru": "^1.0.2",
    "postcss": "^7.0.14",
    "postcss-less": "^3.1.2",
    "pouchdb": "^7.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "xml": "^1.0.1"
  },
  "dependencies": {
    "@types/parse5": "^5.0.0",
    "@types/request": "^2.48.1",
    "babylon": "^6.18.0",
    "map-lru": "^1.0.2",
    "opn": "^4.0.2",
    "postcss": "^7.0.14",
    "postcss-less": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:fuyingjun/mx-plugin.git"
  },
  "author": "",
  "license": "ISC"
}
